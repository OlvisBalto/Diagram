<?xml version="1.0" encoding="utf-8" ?>
<configuration>	
	<startup>
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
	</startup>
	<system.serviceModel>
		<services>
			<service name="TensionDiagramService.DisplayTension" >
				<!-- TCP endpoint -->
				
				<host>
					<baseAddresses>
						<add baseAddress="net.tcp://localhost:30000/TensionDiagramService.DisplayTension"/>
					</baseAddresses>
				</host>
			</service>
		</services>
		
	</system.serviceModel>
</configuration>